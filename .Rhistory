fileLocation<-"Z:/Shared/Assay Development/AssayTeam_ExperimentalData/Kamin/AssayExperiments/20180307_Enzyme_Storage_pH/ALL_conditions_TTR.csv"
TTRdata<-as.data.frame(read.table(fileLocation,header=TRUE,sep=',',stringsAsFactors = TRUE))
TTRdata$Spike.Concentration<-factor(TTRdata$Spike.Concentration, levels = unique(TTRdata$Spike.Concentration[order(TTRdata$Spike.Concentration)]))
TTRdata$Enzyme_pH <- factor(TTRdata$Enzyme_pH, levels = unique(TTRdata$Enzyme_pH))
TTRdata$Storage_Time <- factor(TTRdata$Storage_Time, levels = unique(TTRdata$Storage_Time))
levels(TTRdata$Spike.Concentration) <- c("NTC", "565 copies/rxn", "1130 copies/rxn", "2260 copies/rxn")
p <- ggplot(TTRdata, aes(x = Enzyme_pH, y = TTR, fill = Storage_Time)) +
geom_dotplot(binaxis = 'y', stackdir = 'center', dotsize = 1.2, stackratio = 1, stackgroups = TRUE, binpositions='all') +
facet_wrap(~Spike.Concentration, scales = 'free') +
scale_fill_discrete(name = "Enzyme Storage\nTime (mins)") +
scale_x_discrete(name = "Enzyme Storage pH") +
scale_y_continuous(name = "TTR (mins)")
ggsave("Summary_plot", plot = p, path = folder)
